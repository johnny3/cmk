<h1>NewsletterArticle edit</h1>

<form action="{{ path('newsletter_article_admin_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }} novalidate>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
    {{ tinymce_init() }}
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('newsletter_article_admin') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('newsletter_article_admin_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
